{
  "name": "zw",
  "version": "1.0.0",
  "private": true,
  "description": "ZI & Zsh Wiki",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-shell/zw.git"
  },
  "author": "Salvydas Lukosius",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/z-shell/zw/issues"
  },
  "homepage": "https://github.com/z-shell",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prebuild": "[[ $CI = true ]] && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build:cf": "npx pnpm i --store=node_modules/.pnpm-store && npx run build",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-new-version": "docusaurus docs:version",
    "format:check": "prettier --check '**/*.{js,css,json,md,ts,tsx,yml,yaml}'",
    "format:write": "prettier --write '**/*.{js,css,json,md,ts,tsx,yml,yaml}'",
    "format:diff": "prettier --list-different . ",
    "crowdin": "crowdin",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.16",
    "@docusaurus/plugin-ideal-image": "^2.0.0-beta.16",
    "@docusaurus/plugin-pwa": "^2.0.0-beta.16",
    "@docusaurus/preset-classic": "2.0.0-beta.16",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@algolia/client-search": "4.9.1",
    "@crowdin/cli": "3.7.8",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.16",
    "@tsconfig/docusaurus": "1.0.4",
    "@types/react": "16.8.0",
    "prettier": "2.5.1",
    "prop-types": "^15.8.1",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "@algolia/client-search": "4.9.1",
    "@types/react": "16.8.0"
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=3",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  }
}
