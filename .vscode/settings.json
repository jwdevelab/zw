{
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": true
  },
  "markdownlint.config": {
    "MD033": false,
    "MD029": false
  },
  "markdownlint.ignore": [
    "ignored.md"
  ],
  "conventionalCommits.scopes": [
    "workspace"
  ]
}
